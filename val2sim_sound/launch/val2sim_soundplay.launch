<?xml version="1.0"?>
<launch>
    <arg name="device" default="" />

    <node name="soundplay_node" pkg="sound_play" type="soundplay_node.py">
        <param name="device" value="$(arg device)" />
    </node>

    <!--SETUP SILENT SUBSCRIBER NODE-->
    <node name="val2sim_silentplay_node"   pkg="val2sim_sound"    type="val2sim_silentplay.py" />

    <!--SETUP TOUCH SUBSCRIBER NODE-->
    <node name="val2sim_touchplay_node"   pkg="val2sim_sound"    type="val2sim_touchplay.py" />

</launch>
